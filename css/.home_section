body
{
	background-color: white;
	font: Raleway;
	margin:  0;
}
.wrapper
{
	max-width: 1500px;
	margin: auto;
}

.home_section
{
	background-image: url('image/banner.png');
	background-size: 100% 100%;
	width: 100%;
   	height: 850px;
}
.icon_unicorn
{
	font-size: 14px;
    float: left;
    margin-left: 150px;
    margin-top: 100px;
}
.menu_icon {
    display: inline-block;
    width: 45px;
    height: 35px;
    margin-right: 150px;
    margin-top: 100px;
    float: right;
}
.menu_icon span {
    display: block;
    height: 2px;
    width: 20px;
    background: black;
    cursor: pointer;
}
.menu_icon span:nth-child(1) {
    margin-top: 0px;
}

.menu_icon span:nth-child(2), .menu_icon span:nth-child(3) {
    margin-top: 5px;
}
